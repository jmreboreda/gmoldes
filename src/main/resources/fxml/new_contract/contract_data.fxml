            <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES" />
            <RowConstraints />
         </rowConstraints>
         <children>
            <Label text="Tipo" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </Label>
            <ChoiceBox fx:id="contractType" prefHeight="26.0" prefWidth="709.0" GridPane.columnIndex="1" GridPane.columnSpan="7" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </ChoiceBox>
            <TextField fx:id="descriptionContractOther" editable="false" prefHeight="26.0" prefWidth="379.0" promptText="Descripción del tipo de contrato &quot;Otros tipos de contrato sin descripción específica&quot;" GridPane.columnIndex="1" GridPane.columnSpan="7" GridPane.rowIndex="2">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </TextField>
            <Label text="Duración" GridPane.rowIndex="3">
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </Label>
            <ContractDuration fx:id="contractDuration" GridPane.columnIndex="1"  GridPane.rowIndex="3" GridPane.columnSpan="8">
            </ContractDuration>
            <Label text="Jornada" GridPane.rowIndex="4">
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </Label>
            <!--<CustomInputHoursWorkWeek fx:id="hoursWorkWeek" alignment="CENTER" GridPane.columnIndex="4" GridPane.columnSpan="4" GridPane.rowIndex="4">-->
            <!--</CustomInputHoursWorkWeek>-->
            <TextInput fx:id="hoursWorkWeek" alignment="CENTER" GridPane.columnIndex="4" GridPane.columnSpan="4" GridPane.rowIndex="4">

            </TextInput>
            <Label text="Días Jornada" wrapText="true" GridPane.rowIndex="5">
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Label>
            <DaysOfWeekSelector fx:id="daysOfWeekToWorkSelector" alignment="CENTER" spacing="25.0" GridPane.columnIndex="1" GridPane.columnSpan="7" GridPane.rowIndex="5">
