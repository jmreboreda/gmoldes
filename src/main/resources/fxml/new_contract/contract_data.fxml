               </GridPane.margin>
            </Label>
            <DateInput fx:id="dateNotification" alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.columnSpan="2">
            </DateInput>
            <TextField fx:id="hourNotification" alignment="CENTER_LEFT" promptText="hh:mm" GridPane.columnIndex="2">
            </TextField>
            <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="4">
               <children>
                  <RadioButton fx:id="radioButtonFullWorkDay" maxHeight="18.0" maxWidth="100.0" minHeight="18.0" minWidth="100.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="100.0" selected="true" text="Completa">
                     <HBox.margin>
                        <Insets top="12.0" />
                     </HBox.margin>
                     <toggleGroup>
                        <ToggleGroup fx:id="grWorkDay" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton fx:id="radioButtonPartialWorkDay" mnemonicParsing="false" text="Tiempo parcial" toggleGroup="$grWorkDay">
                     <HBox.margin>
                        <Insets left="15.0" top="12.0" />
                     </HBox.margin>
                  </RadioButton>
               </children>
            </HBox>
