<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <Group fx:id="contractExtensionGroup">
               <children>
                  <HBox>
                     <children>
                        <Label prefHeight="30.0" text="Fecha desde">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </Label>
                        <DatePicker fx:id="dateFrom" prefHeight="30.0" prefWidth="140.0">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </DatePicker>
                        <Label prefHeight="30.0" text="Fecha hasta">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </Label>
                        <DatePicker fx:id="dateTo" prefHeight="30.0" prefWidth="140.0">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </DatePicker>
                        <Label prefHeight="30.0" text="Duración de la prórroga">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin>
                        </Label>
                        <TextField fx:id="contractExtensionDuration" alignment="CENTER" maxWidth="125.0" prefHeight="30.0" />
                     </children>
                  </HBox>
               </children>
            </Group>
         </children>
      </VBox>
   </children>
</fx:root>
