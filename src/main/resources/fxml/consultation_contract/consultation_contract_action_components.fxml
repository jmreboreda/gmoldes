<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox AnchorPane.rightAnchor="5.0">
         <children>
            <Label minHeight="70.0" wrapText="true">
               <HBox.margin>
                  <Insets top="15.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="okButton" alignment="CENTER" contentDisplay="CENTER" disable="true" maxHeight="70.0" maxWidth="80.0" minHeight="70.0" minWidth="110.0" mnemonicParsing="false" prefHeight="70.0" prefWidth="80.0" text="Aceptar" textAlignment="CENTER" textFill="#02430e" wrapText="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <Button fx:id="saveButton" alignment="CENTER" contentDisplay="CENTER" disable="true" maxHeight="70.0" maxWidth="80.0" minHeight="70.0" minWidth="110.0" mnemonicParsing="false" prefHeight="70.0" prefWidth="80.0" text="Guardar" textAlignment="CENTER" textFill="#000fff" wrapText="true">
               <font>
                  <Font size="13.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" right="20.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="exitButton" alignment="CENTER" contentDisplay="CENTER" maxHeight="70.0" maxWidth="80.0" minHeight="70.0" minWidth="110.0" mnemonicParsing="false" prefHeight="70.0" prefWidth="80.0" text="Salir" textAlignment="CENTER" textFill="#640000">
               <HBox.margin>
                  <Insets bottom="10.0" right="5.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets left="50.0" />
         </padding>
      </HBox>
   </children>
</fx:root>
