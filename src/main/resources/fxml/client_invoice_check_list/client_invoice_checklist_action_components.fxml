<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root type="HBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label prefHeight="70.0" prefWidth="400.0" text="Para usar con la plantilla: DGM_014_Clientes_reclamación_documentación_mensual" wrapText="true">
         <HBox.margin>
            <Insets top="15.0" />
         </HBox.margin>
      </Label>
      <Button fx:id="clipboardCopyButton" alignment="CENTER" contentDisplay="CENTER" maxHeight="70.0" maxWidth="80.0" minHeight="70.0" minWidth="110.0" mnemonicParsing="false" prefHeight="70.0" prefWidth="80.0" text="Copiar al portapapeles" textAlignment="CENTER" textFill="#000fff" wrapText="true">
         <HBox.margin>
            <Insets bottom="15.0" left="20.0" right="20.0" top="15.0" />
         </HBox.margin>
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" alignment="CENTER" contentDisplay="CENTER" maxHeight="70.0" maxWidth="80.0" minHeight="70.0" minWidth="110.0" mnemonicParsing="false" prefHeight="70.0" prefWidth="80.0" text="Salir" textAlignment="CENTER" textFill="#640000">
         <HBox.margin>
            <Insets bottom="15.0" right="20.0" top="15.0" />
         </HBox.margin>
         <font>
            <Font size="13.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets left="50.0" />
   </padding>
</fx:root>
